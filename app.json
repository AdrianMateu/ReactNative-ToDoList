{
	"expo": {
		"name": "ToDoList",
		"slug": "ToDoList",
		"privacy": "public",
		"sdkVersion": "36.0.0",
		"platforms": ["ios", "android", "web"],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#0066ff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adrianmateu.todolist",
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesario para acceder a tu posici√≥n"
      }
		},
		"description": "",
		"android": {
			"package": "com.adrianmateu.todolist",
			"versionCode": 1,
			"permissions": [
				"ACCESS_COARSE_LOCATION",
				"ACCESS_FINE_LOCATION",
				"READ_EXTERNAL_STORAGE",
				"WRITE_EXTERNAL_STORAGE"
			]
		}
	}
}
